db.sequence.insert({_id: "category",seq: 0})
db.sequence.insert({_id: "product",seq: 0})
db.sequence.insert({_id: "variant",seq: 0})
db.sequence.insert({_id: "user",seq: 0})
db.sequence.insert({_id: "order",seq: 0})
db.sequence.insert({_id: "orderPoint",seq: 0})
db.sequence.insert({_id: "delivery",seq: 0})
db.sequence.insert({_id: "payment",seq: 0})


db.createCollection("category", {autoIndexId:true} )
db.createCollection("product", {autoIndexId:true} )
db.createCollection("variant", {autoIndexId:true} )
db.createCollection("order", {autoIndexId:true} )
